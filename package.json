{
  "name": "serverless-agents",
  "version": "0.1.0",
  "description": "A production-ready starter for building AI applications with serverless agents on Vercel, Inngest workflows, and Next.js",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "biome check --write",
    "lint:check": "biome check",
    "format": "biome format --write",
    "format:check": "biome format",
    "mexc-agent": "python start_mexc_agent.py",
    "mexc-agent-dev": "python start_mexc_agent.py --port 8001 --host localhost",
    "mexc-test": "python test_mexc_agent.py",
    "mexc-check": "python start_mexc_agent.py --check-only",
    "setup:turso": "./scripts/setup-turso.sh",
    "lint:all": "./scripts/lint-and-format.sh",
    "type-check": "tsc --noEmit",
    "pre-commit": "bun run lint:all && bun run type-check"
  },
  "dependencies": {
    "@libsql/client": "^0.15.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@vercel/blob": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "inngest": "^3.38.0",
    "lucide-react": "^0.511.0",
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "swr": "^2.2.5",
    "tailwind-merge": "^3.3.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.2",
    "typescript": "^5"
  }
}
